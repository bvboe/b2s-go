{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "plugins": ["html"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "script"
  },
  "rules": {
    "indent": "off",
    "linebreak-style": ["error", "unix"],
    "quotes": "off",
    "semi": ["warn", "always"],
    "no-unused-vars": "off",
    "no-console": "off",
    "no-undef": "warn",
    "no-redeclare": "off"
  }
}
