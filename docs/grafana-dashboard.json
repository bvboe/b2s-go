      "id": 23,
      "panels": [],
      "title": "Top Affected Images",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Critical" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "basic", "type": "color-background" } },
              { "id": "color", "value": { "fixedColor": "dark-red", "mode": "fixed" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "High" },
            "properties": [
              { "id": "custom.cellOptions", "value": { "mode": "basic", "type": "color-background" } },
              { "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }
            ]
          }
        ]
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 33 },
      "id": 24,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Critical" }]
      },
      "targets": [
        {
          "expr": "topk(20, sum by (image_repo, image_tag) (bjorn2scan_vulnerability{deployment_uuid=~\"$deployment\", namespace=~\"$namespace\", severity=\"Critical\"}))",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "Critical"
        },
        {
          "expr": "topk(20, sum by (image_repo, image_tag) (bjorn2scan_vulnerability{deployment_uuid=~\"$deployment\", namespace=~\"$namespace\", severity=\"High\"}))",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "High"
        }
      ],
      "title": "Top 20 Images by Critical/High Vulnerabilities",
      "transformations": [
        {
          "id": "seriesToColumns",
          "options": { "byField": "image_repo" }
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Time 1": true,
              "Time 2": true
            },
            "indexByName": {
              "image_repo": 0,
              "image_tag 1": 1,
              "Value #Critical": 2,
              "Value #High": 3
            },
            "renameByName": {
              "image_repo": "Image",
              "image_tag 1": "Tag",
              "Value #Critical": "Critical",
              "Value #High": "High"
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "collapsed": false,
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 43 },
      "id": 30,
      "panels": [],
      "title": "Known Exploited Vulnerabilities (CISA KEV)",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [{ "color": "green", "value": null }]
          }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "severity" },
            "properties": [
              { "id": "custom.width", "value": 80 },
              {
                "id": "mappings",
                "value": [
                  { "options": { "Critical": { "color": "dark-red", "index": 0 } }, "type": "value" },
                  { "options": { "High": { "color": "orange", "index": 1 } }, "type": "value" },
                  { "options": { "Medium": { "color": "yellow", "index": 2 } }, "type": "value" }
                ]
              },
              { "id": "custom.cellOptions", "value": { "mode": "basic", "type": "color-background" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Value" },
            "properties": [{ "id": "custom.width", "value": 60 }]
          }
        ]
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 44 },
      "id": 31,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": true, "displayName": "Value" }]
      },
      "targets": [
        {
          "expr": "topk(50, bjorn2scan_vulnerability_exploited{deployment_uuid=~\"$deployment\", namespace=~\"$namespace\", severity=~\"$severity\"})",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Known Exploited Vulnerabilities (CISA KEV) - Top 50",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "deployment_uuid": true,
              "deployment_uuid_host_name": true,
              "deployment_uuid_namespace": true,
              "deployment_uuid_namespace_image": true,
              "deployment_uuid_namespace_image_id": true,
              "deployment_uuid_namespace_pod": true,
              "deployment_uuid_namespace_pod_container": true,
              "image_digest": true,
              "instance": true,
              "instance_type": true,
              "job": true,
              "vulnerability_id": true,
              "host_name": true,
              "distro": true,
              "image_repo": true,
              "image_tag": true
            },
            "indexByName": {
              "severity": 0,
              "vulnerability": 1,
              "namespace": 2,
              "pod": 3,
              "container": 4,
              "package_name": 5,
              "package_version": 6,
              "fix_status": 7,
              "fixed_version": 8
            },
            "renameByName": {}
          }
        }
      ],
