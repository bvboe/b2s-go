        "orientation": "horizontal",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showUnfilled": true
      },
      "targets": [
        {
          "expr": "topk(10, max by (vulnerability, severity) (bjorn2scan_vulnerability_risk{deployment_uuid=~\"$deployment\", namespace=~\"$namespace\", severity=~\"$severity\"}))",
          "legendFormat": "{{vulnerability}} ({{severity}})",
          "refId": "A"
        }
      ],
      "title": "Top 10 Highest Risk Vulnerabilities",
      "type": "bargauge"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "continuous-RdYlGr", "reverse": true },
          "mappings": [],
          "thresholds": {
